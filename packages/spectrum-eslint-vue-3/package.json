{
  "name": "eslint-plugin-spectrum-eslint-vue-3",
  "version": "0.1.7",
  "description": "Custom lint plugin",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "ООО \"АВТОРАПОРТ\" (https://spectrumdata.ru/)",
  "main": "./dist/index.js",
  "exports": "./dist/index.js",
  "module": "./dist/index.js",
  "files": [
    "/dist",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "build": "rm -rf ./dist && mkdir ./dist && cp -r ./lib/* ./dist",
    "pack": "npm run clean:tgz && npm pack --pack-destination=./dist",
    "lint": "npm-run-all \"lint:*\"",
    "lint:eslint-docs": "npm-run-all \"update:eslint-docs -- --check\"",
    "lint:js": "eslint .",
    "test": "mocha tests --recursive",
    "update:eslint-docs": "eslint-doc-generator"
  },
  "dependencies": {
    "requireindex": "^1.2.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-doc-generator": "^1.6.1",
    "eslint-plugin-eslint-plugin": "^5.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-vue": "^9.19.2",
    "mocha": "^10.0.0",
    "npm-run-all": "^4.1.5"
  },
  "engines": {
    "node": "^14.17.0 || ^16.0.0 || >= 18.0.0"
  },
  "peerDependencies": {
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2"
  },
  "peerDependenciesMeta": {
    "eslint": {
      "optional": false
    },
    "eslint-plugin-vue": {
      "optional": false
    }
  },
  "license": "UNLICENSED"
}
